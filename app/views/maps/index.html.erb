<p id="notice"><%= notice %></p>


<h1>Ваши карты</h1>

  <table class="table table-condensed">
    <thead>
      <tr>
        <th>Радиус области</th>
        <th>Широта</th>
        <th>Долгота</th>
        <th>Адрес</th>
        <th colspan="3"></th>
      </tr>
    </thead>
    <tbody>

      <% @maps.each do |map| %>
      <tr>
        <td><%= map.radius %></td>
        <td><%= map.latitude %></td>
        <td><%= map.longtitude %></td>
        <td><%= map.address %></td>
        <td>
          <%= link_to 'Show', map, class: "btn btn-primary" %>
          <%= link_to 'Destroy', map, method: :delete, data: { confirm: 'Are you sure?' },class: "btn btn-warning" %>
        </td>
      </tr>
    <% end %>
    </tbody>
  </table>
